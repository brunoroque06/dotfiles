[
  {
    "bindings": {
      "cmd-enter": "editor::ToggleCodeActions",
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward",
      "cmd-b": "file_finder::Toggle",
      "cmd-o": "file_finder::Toggle",
      "cmd-p": "command_palette::Toggle",
      "cmd-r": ["vim::Search", "search::ToggleReplace"],
      "cmd-shift-r": ["vim::Search", "search::ToggleReplace"],
      "cmd-t": "terminal_panel::ToggleFocus",
      "cmd-y": "outline::Toggle",
      "cmd-shift-y": "project_symbols::Toggle"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "==": "editor::Format"
    }
  }
]
